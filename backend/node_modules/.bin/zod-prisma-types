#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tareque/projects/newsletter-monorepo/backend/node_modules/.pnpm/zod-prisma-types@3.2.4_@prisma+client@6.8.2_prisma@6.8.2_typescript@5.8.3__typescript@5_34ca71fc8c155054c95a4e65c070229f/node_modules/zod-prisma-types/dist/node_modules:/Users/tareque/projects/newsletter-monorepo/backend/node_modules/.pnpm/zod-prisma-types@3.2.4_@prisma+client@6.8.2_prisma@6.8.2_typescript@5.8.3__typescript@5_34ca71fc8c155054c95a4e65c070229f/node_modules/zod-prisma-types/node_modules:/Users/tareque/projects/newsletter-monorepo/backend/node_modules/.pnpm/zod-prisma-types@3.2.4_@prisma+client@6.8.2_prisma@6.8.2_typescript@5.8.3__typescript@5_34ca71fc8c155054c95a4e65c070229f/node_modules:/Users/tareque/projects/newsletter-monorepo/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tareque/projects/newsletter-monorepo/backend/node_modules/.pnpm/zod-prisma-types@3.2.4_@prisma+client@6.8.2_prisma@6.8.2_typescript@5.8.3__typescript@5_34ca71fc8c155054c95a4e65c070229f/node_modules/zod-prisma-types/dist/node_modules:/Users/tareque/projects/newsletter-monorepo/backend/node_modules/.pnpm/zod-prisma-types@3.2.4_@prisma+client@6.8.2_prisma@6.8.2_typescript@5.8.3__typescript@5_34ca71fc8c155054c95a4e65c070229f/node_modules/zod-prisma-types/node_modules:/Users/tareque/projects/newsletter-monorepo/backend/node_modules/.pnpm/zod-prisma-types@3.2.4_@prisma+client@6.8.2_prisma@6.8.2_typescript@5.8.3__typescript@5_34ca71fc8c155054c95a4e65c070229f/node_modules:/Users/tareque/projects/newsletter-monorepo/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../zod-prisma-types/dist/bin.js" "$@"
else
  exec node  "$basedir/../zod-prisma-types/dist/bin.js" "$@"
fi
